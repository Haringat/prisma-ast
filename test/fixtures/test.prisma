datasource db {
  provider = "postgres"
  url      = env("DATABASE_URL")
}

// this is foo
generator foo {
  // it is a nexus
  provider = "nexus"
}

model Bar {
  id   Int    @id @first(fields: [one, two]) @second(fields: ["three", "four"])
  name String @unique

  supercalifragilisticexpialidocious String @default("it is something quite atrocious")
  owner                              String
}

enum Role {
  USER
  ADMIN
}
